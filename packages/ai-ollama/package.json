{
  "name": "@tanstack/ai-ollama",
  "version": "0.1.0",
  "description": "Ollama adapter for TanStack AI",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist node_modules",
    "typecheck": "tsc --noEmit",
    "lint": "tsc --noEmit"
  },
  "keywords": [
    "ai",
    "ollama",
    "llama",
    "mistral",
    "tanstack",
    "adapter"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@tanstack/ai": "workspace:*",
    "ollama": "^0.5.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "tsup": "^8.0.2",
    "typescript": "^5.4.2"
  },
  "peerDependencies": {
    "@tanstack/ai": "workspace:*"
  }
}
